<Window x:Class="Komissarov.Nsu.OracleClient.Views.ReportView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Report">
    <Grid>
        <DockPanel>
            <TextBlock DockPanel.Dock="Top" Text="Query successfully executed" Margin="10"/>
            <DataGrid DockPanel.Dock="Bottom" ItemsSource="{Binding DataReader}" AutoGenerateColumns="True" AutoGeneratingColumn="DataGrid_AutoGeneratingColumn"/>    
        </DockPanel>
        
    </Grid>
</Window>
